{
  "name": "mock-api-server",
  "version": "1.0.0",
  "description": "Mock API server for Biteship, Xendit, etc. - for development/testing",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc && npx prisma generate",
    "start": "npx prisma db push && node dist/server.js",
    "db:push": "npx prisma db push",
    "db:seed": "npx ts-node prisma/seed.ts",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": ["mock", "api", "biteship", "xendit", "shipping"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "prisma": "^5.22.0",
    "typescript": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1"
  }
}
